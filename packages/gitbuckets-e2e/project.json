{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/gitbuckets-e2e/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/gitbuckets-e2e/**/*.ts"],
        "maxWarnings": 0
      }
    },
    "e2e-test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/gitbuckets-e2e"],
      "options": {
        "jestConfig": "packages/gitbuckets-e2e/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:e2e", "scope:gitbuckets"]
}
